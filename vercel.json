{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key"
  },
  "routes": [
    {
      "src": "^/(?!api).*$",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
